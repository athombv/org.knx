{
  "type": "number",
  "title": {
    "en": "Actuator value"
  },
  "units": {
    "en": "%",
    "nl": "%",
    "da": "%",
    "it": "%",
    "ko": "%",
    "pl": "%",
    "ru": "%",
    "sv": "%",
    "es": "%",
    "fr": " %",
    "no": "%"
  },
  "desc": {
    "en": "The current actuator value in percent."
  },
  "insights": false,
  "chartType": "stepLine",
  "decimals": 2,
  "min": 0,
  "max": 255,
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "actuator_value_changed",
        "title": {
          "en": "The actuator value changed"
        },
        "tokens": [
          {
            "name": "$id",
            "title": "$title",
            "type": "$type",
            "example": 50
          }
        ]
      }
    ]
  },
  "minCompatibility": "5.0.0"
}
